{
  "ignore_labels": ["ignore in changelog", "bump version"],
  "template": "Changelog\n\n#{{CHANGELOG}}",
  "pr_template": "- #{{TITLE}}\n\t- Author: #{{AUTHOR}}",
  "categories": [
    {
      "title": "Features/Enhancements",
      "labels": ["enhancement"]
    },
    {
      "title": "Bug Fixes",
      "labels": ["bug"]
    },
    {
      "title": "Refactor",
      "labels": ["refactor"]
    },
    {
      "title": "Dependencies",
      "labels": ["dependencies"]
    },
    {
      "title": "Styling",
      "labels": ["styling"]
    },
    {
      "title": "Documentation",
      "labels": ["documentation"]
    },
    {
      "title": "CICD",
      "labels": ["cicd"]
    }
  ]
}
